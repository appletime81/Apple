<?xml version="1.0" ?>
<resource xmlns="http://www.wxwidgets.org/wxxrc">
    <object class="wxFrame" name="root">
        <title>Mizuho Analysis Tool</title>
        <icon>icon/launcher.ico</icon>
        <size>1120, 630</size>  <!-- 16:9 -->
        <!-- Min size - cannot set in XRC -->
        <style>wxDEFAULT_FRAME_STYLE</style>

        <object class="wxPanel">
            <object class="wxBoxSizer">
                <orient>wxVERTICAL</orient>

                <object class="sizeritem">
                    <flag>wxEXPAND</flag>
                    <option>1</option>

                    <object class="wxNotebook" name="notebook" />
                </object>
            </object>
        </object>

    </object>
</resource>
